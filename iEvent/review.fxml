      <TextField fx:id="categoryfield" layoutX="293.0" layoutY="140.0" prefHeight="33.0" prefWidth="401.0" promptText="categorie" />
      <Button fx:id="Retour" layoutX="87.0" layoutY="516.0" mnemonicParsing="false" onAction="#Retour" prefHeight="31.0" prefWidth="124.0" text="Retour" textFill="#432cee" />
